/**STORE:
JACKETS(2)
SHORTS(5)
SHIRTS(3)
PANTS(5)
**/

CREATE TABLE STORE (ID INTEGER PRIMARY KEY, NAME TEXT, ONLINE TEXT, QUANTITY INTEGER, PRICE INTEGER);

INSERT INTO STORE VALUES
(1, "JACKETS","NO",2,25),
(2, "SHORTS", "YES",5,10),
(3,"SHIRTS", "NO", 3,13),
(4,"PANTS","YES",5,15)
;
SELECT * FROM STORE;

SELECT * FROM STORE
WHERE PRICE>=10
ORDER BY PRICE ASC;

SELECT SUM(QUANTITY)FROM STORE
